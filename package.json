{
  "name": "companies-house-frontend",
  "version": "1.0.0",
  "description": "Filter facility frontend",
  "main": "pages/_app.js",
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start -p 8080",
    "format": "prettier --write **/**.{ts,tsx,css,scss}",
    "test": "npm run test:unit && npm run test:browser",
    "test:unit": "mocha -r ts-node/register tests/unit/**/*.spec.ts",
    "test:browser:skipbuild:upload": "set SKIP_BUILD=1 && set UPLOAD_TESTS=1 && npm run test:browser",
    "test:browser": "playwright test --reporter=json,line"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "post-commit": "set SKIP_BUILD=1 && set UPLOAD_TESTS=1 && npm run test:browser"
    }
  },
  "keywords": [
    "companies-house",
    "accounts",
    "filter-facility"
  ],
  "homepage": "https://filterfacility.co.uk",
  "author": "Brian Evans",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@companieshouse/api-sdk-node": "^1.0.25",
    "@google-cloud/storage": "^5.8.5",
    "camelcase": "^6.2.0",
    "camelcase-keys": "^6.2.2",
    "fast-csv": "^4.3.6",
    "next": "^10.2.3",
    "next-auth": "^3.23.0",
    "object-hash": "^2.1.1",
    "pg": "^8.6.0",
    "pg-query-stream": "^4.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "requests": "^0.3.0",
    "sass": "^1.32.8",
    "snakecase-keys": "^4.0.2",
    "swr": "^0.5.6",
    "to-snake-case": "^1.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.12.2",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "@types/object-hash": "^2.1.0",
    "@types/pg": "^8.6.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "husky": "^6.0.0",
    "mocha": "^9.0.1",
    "playwright": "^1.12.2",
    "prettier": "^2.2.1",
    "request": "^2.88.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3",
    "webpack": "^5.38.1"
  }
}
